

<div class="row justify-content-center align-items-center mt-5">
<%= render @report %>
  <div class="input-group justify-content-center shadow">
    <%= link_to "Editar Reporte", edit_report_path(@report), class: "btn btn-primary" %>
    <%= link_to "Volver a Reportes", reports_path, class: "btn btn-warning" %>
    <%= button_to "Eliminar Reporte", @report, method: :delete, class: "btn btn-primary rounded-0 rounded-end" %>
  </div>
</div>

